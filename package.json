{
  "private": true,
  "name": "lottoland",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./src/index.js",
  "author": "Mario Nunes",
  "bugs": {
    "url": "https://github.com/mnunes01/lottoland.git/issues"
  },
  "homepage": "https://github.com/mnunes01/lottoland.git#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnunes01/lottoland.git"
  },
  "scripts": {
    "build": "preact build",
    "build-no-prerender": "preact build --no-prerender",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest",
    "gh-pages": "git subtree push --prefix build origin gh-pages"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "node-sass": "^6.0.0",
    "preact-cli": "^3.0.0",
    "raw-loader": "^4.0.2",
    "sass-loader": "^10.2.0",
    "sirv-cli": "1.0.3"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "preact": "^10.3.2",
    "preact-custom-element": "^4.2.1",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
